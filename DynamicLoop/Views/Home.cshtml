@using AutoMapper
@using DynamicLoop.Components.Models.Home
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
    var educationModel = Mapper.Map<IPublishedContent, EducationModel>(Model.Content);
    var skillsModel = Mapper.Map<IPublishedContent, SkillsModel>(Model.Content);
    var workExperienceModel = Mapper.Map<IPublishedContent, WorkExperienceModel>(Model.Content);
}

@Html.Partial("Education", educationModel)

@Html.Partial("Skills", skillsModel)

@Html.Partial("WorkExperience", workExperienceModel)   		   
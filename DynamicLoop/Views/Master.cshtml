@using DynamicLoop.Components.Models.Master
@using AutoMapper
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    var metaTagsModel = Mapper.Map<IPublishedContent, MetaTagsModel>(Model.Content);
    var topContentModel = Mapper.Map<IPublishedContent, TopContentModel>(Model.Content);
}

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN""http://www.w3.org/TR/html4/strict.dtd">
<head>
    @Html.Partial("MetaTags",metaTagsModel)
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" >
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/3.7.3/build/cssreset/cssreset-min.css" />
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/3.7.3/build/cssbase/cssbase-min.css" />
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/3.7.3/build/cssfonts/cssfonts-min.css" />
    <link rel="stylesheet" type="text/css" href="/css/styles.css" media="screen">
    <link rel="stylesheet" type="text/css" href="/css/print.css" media="print">

    @RenderSection("Header", false)
</head>
<body>
    <div id="main">
        <div id="topContent">
            @Html.Partial("TopContent", topContentModel)
        </div>
        <div id="middleContent">
            @RenderBody()
        </div>
        <div id="bottomContent">
            @Html.Partial("Footer", metaTagsModel)
        </div>
    </div>

<script src="/scripts/cufon.js" type="text/javascript"></script>
<script src="/scripts/Myriad_Pro_400-Myriad_Pro_900-Myriad_Pro_italic_400-Myriad_Pro_italic_700.font.js" type="text/javascript"></script>
<script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.9.1.js"></script>
<script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script>
<script src="/scripts/custom.js" type="text/javascript"></script>
</body>
</html>